        <attr format="color" name="selectedDayTextColor"/>
        <attr format="color" name="weekLayoutBackgroundColor"/>
        <attr format="color" name="titleLayoutBackgroundColor"/>
        <attr format="color" name="currentDayOfMonthColor"/>
    </declare-styleable>
